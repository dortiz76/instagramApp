<!DOCTYPE html>

<html>
<head>
	<% include ../partials/head %>
<title></title>
</head>
<body class="container">
<header>
<% include ../partials/header %>
</header>

<main>


<div class="row">
	<% grams.forEach(function(gram) { %>
  <div class="col-sm-3 col-md-2">
    <div class="thumbnail">
      <img src="<%=gram.images.standard_resolution.url %>" class="img-responsive">
      <div class="caption">
        
        <p class="navbar-text">Total Likes: <%= gram.likes.count %><br>Total Comments:<%= gram.comments.count %></p>
        <p><a href="<%= gram.link %>" target="_blank" class="btn btn-primary btn-sm" role="button">Read More</a></p>
      </div>
    </div>
  </div>

	
	<% }) %>


</div>
</main>

<footer>
<% include ../partials/footer %>
</footer>

</body>
</html>